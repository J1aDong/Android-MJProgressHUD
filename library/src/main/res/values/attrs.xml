<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="MJCircleProgressBar">
        <attr name="mjprogress_roundColor" format="color"/>
        <attr name="mjprogress_roundProgressColor" format="color"/>
        <attr name="mjprogress_roundWidth" format="dimension"></attr>
        <attr name="mjprogress_max" format="integer"></attr>
        <attr name="mjprogress_style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>